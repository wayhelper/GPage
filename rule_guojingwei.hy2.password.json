{
  "log": {
    "level": "info"
  },
  "dns": {
    "servers": [
      {
        "tag": "google-doh",
        "address": "https://dns.google/dns-query",
        "detour": "direct"
      },
      {
        "tag": "local-fallback",
        "address": "8.8.8.8",
        "detour": "direct"
      }
    ],
    "strategy": "ipv4_only"
  },
  "inbounds": [
    {
      "type": "socks",
      "tag": "socks-in",
      "listen": "127.0.0.1",
      "listen_port": 1080,
      "sniff": true,
      "sniff_override_destination": true
    }
  ],
  "outbounds": [
    {
      "type": "hysteria2",
      "tag": "proxy-out",
      "server": "i.alal.site",
      "server_port": 9527,
      "password": "guojingwei",
      "tls": {
        "enabled": true,
        "server_name": "i.alal.site"
      }
    },
    {
      "type": "direct",
      "tag": "direct"
    },
    {
      "type": "block",
      "tag": "block"
    }
  ],
  "route": {
    "geoip": {
      "path": "geoip.db"
    },
    "geosite": {
      "path": "geosite.db"
    },
    "rules": [
      {
        "geosite": "cn",
        "geoip": "cn",
        "outbound": "direct"
      },
      {
        "geosite": "geolocation-!cn",
        "outbound": "proxy-out"
      },
      {
        "ip_cidr": ["geoip:private"],
        "outbound": "direct"
      },
      {
        "outbound": "proxy-out"
      }
    ]
  }
}
